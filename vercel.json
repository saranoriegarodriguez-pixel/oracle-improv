{
  "rewrites": [
    {
      "source": "/app/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/es/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/en/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
